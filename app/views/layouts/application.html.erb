<!DOCTYPE html>
<html>
<head>
  <title>Telos Digital Engagement</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'application', 'https://cdnjs.cloudflare.com/ajax/libs/normalize/3.0.3/normalize.min.css' %>
  <%= stylesheet_link_tag    'application', 'https://www.google.com/fonts#UsePlace:use/Collection:Cinzel:400,700' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-51775216-1', 'auto');
    ga('send', 'pageview');
  </script>
  <style><%= yield :stylesheets %></style>
</head>
<body>

  <%= render "/layouts/nav" %>

  <div class ="container" id="main-container">
    <% flash.each do |name, msg| %>
        <%= content_tag :div, msg, class: "alert alert-info" %>
    <% end %>

    <%= yield %>

  </div> <!--container-->
  <footer class="section" id="main-footer">
    <div class="container">
      <span class="pull-left">Linked In: <a href="https://www.linkedin.com/in/koanbrown">Koan Brown</a></span>
      <span class="pull-right">Github: <a href="https://github.com/telos-io">Telos Digital Engagement</a></span>
    </div>
  </footer>

</body>
</html>
